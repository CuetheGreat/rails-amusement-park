<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_tag '/signin', method: :post do %>
  <div class="field">
    <%= label_tag :name%>
    <%= collection_select :user, :name ,User.all, :name, :name %>
  </div>
  <div class="field">
    <%= label_tag :password%>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag 'Sign In'%>
<%end%>
